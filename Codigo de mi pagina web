        strlen($_POST['forma']) >= 1 &&
        strlen($_POST['sabor']) >= 1 &&
        strlen($_POST['relleno']) >= 1 &&
        strlen($_POST['cobertura']) >= 1 &&
        strlen($_POST['decoración']) >= 1 &&
        strlen($_POST['descripción']) >= 1 &&
